<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddFriendController">
   <children>
      <TextField fx:id="friendUsername" layoutX="104.0" layoutY="41.0" prefHeight="25.0" prefWidth="251.0" promptText="Enter friend username" />

      <Label fx:id="addFriendLabel" layoutX="26.0" layoutY="45.0" text="Username" />

       <Text fx:id="addFriendMsg" layoutX="34.0" layoutY="94.0" text="" visible="false" wrappingWidth="176.0" />

      <Button fx:id="addFriendSubmit" layoutX="280.0" layoutY="98.0" mnemonicParsing="false" onAction="#submitRequest" text="Add Friend" />
      <Button fx:id="cancelAddFriend" layoutX="210.0" layoutY="98.0" mnemonicParsing="false" onAction="#cancelRequest" text="Cancel" />
   </children>
</AnchorPane>
