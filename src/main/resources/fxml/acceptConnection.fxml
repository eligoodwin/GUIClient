<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<?import javafx.geometry.Insets?>
<AnchorPane prefHeight="100.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AcceptConnectionController">
    <children>
      <GridPane>
          <padding><Insets top="20" left="30" right="30" bottom="20"/></padding>
          <Button fx:id="ignoreConnection" layoutX="206.0" layoutY="83.0" mnemonicParsing="false" onAction="#ignoreConnection" text="Ignore" GridPane.RowIndex="1" GridPane.rowSpan="2" GridPane.columnIndex="1" GridPane.halignment="CENTER"/>
          <Button fx:id="acceptConnection" defaultButton="true" layoutX="50.0" layoutY="83.0" mnemonicParsing="false" onAction="#acceptConnection" text="Accept" GridPane.RowIndex="1" GridPane.rowSpan="2" GridPane.columnIndex="0" GridPane.halignment="CENTER"/>
          <Text fx:id="incomingConnectionText" layoutX="24.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Accept connection from ..." GridPane.RowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER"/>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
    </children>
</AnchorPane>
